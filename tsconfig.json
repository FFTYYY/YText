{
	"compilerOptions": {
		"target": "ESNext",
		"useDefineForClassFields": true,
		"lib": ["DOM", "DOM.Iterable", "ESNext"],
		"allowJs": false,
		"skipLibCheck": false,
		"esModuleInterop": false,
		"allowSyntheticDefaultImports": true,
		"strict": false,
		"forceConsistentCasingInFileNames": true,
		"module": "ESNext",
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"jsx": "react-jsx"
	},
	"include": ["./src"] , 

	"typedocOptions": {
		"entryPoints": [
			"src/editor/utils.tsx" , 
			"src/editor/core/elements.tsx" , 
			"src/editor/core/editor/editor_core.tsx" , 
			"src/editor/core/editor/editor_interface.tsx" , 
			"src/editor/components/groups.tsx" , 
			"src/editor/components/hidden.tsx" , 
			"src/editor/components/supports.tsx" , 
			"src/editor/components/universe.tsx" , 
		],
		"out": "docs" , 
	} ,
}
